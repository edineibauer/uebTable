<span class='padding-small transition-easy radius color-white z-depth-2 left' id="{{id}}" style='margin: 2px; height: 30px; overflow: hidden;'>
    {{column}}
    <small>{{operator}}</small>
    <span class="value">{{value}}</span>
    <span class="right padding-left color-gray pointer hover-shadow radius padding-small font-small btn-badge-remove"
          style="height: 22px;width: 22px;float: right;margin: 0 0 0 10px;"
          data-badge="{{id}}" rel="{{identificador}}">
        <i class="material-icons left font-medium" style="margin-left: -12px;">close</i>
    </span>
</span>